def F(b, c):
    D = b**2 - 4 * c
    if D == 0:
        x = -b / 2
        return(x)
    else:
        if b > 0:
            x1 = (-b - D**0.5)/2
            x2 = c/x1
            return(x1,x2)
        else:
            x1 = (-b + D**0.5)/2
            x2 = c/x1
            return(x1,x2)
            
            
print(F(2,1))                 #-0.1
print(F(10,7))                #(-9.242640687119284, -0.757359312880715)  
print(F(0.5,4))               #((-0.2500000000000001-1.984313483298443j), (-0.25000000000000006+1.9843134832984428j))    
print(F(1,1))                 #((-0.5-0.8660254037844386j), (-0.5000000000000001+0.8660254037844387j))  
print(F(100000000000000,1))   #(-100000000000000.0, -1e-14)  
                                                                                                                       
                                                                                  
                                  
                                                   
